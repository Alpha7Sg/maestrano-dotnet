#!/bin/bash
xbuild /p:Configuration=Release src/Maestrano.sln && mono ./src/packages/NUnit.Runners.2.6.4/tools/nunit-console.exe ./src/Maestrano.Tests/bin/Debug/Maestrano.Tests.dll -fixture:Maestrano.Tests.Saml.CertificateTests -stoponerror -trace=Verbose -output=/tmp/.nunit-output
cat /tmp/.nunit-output
rm -f /tmp/.nunit-output
